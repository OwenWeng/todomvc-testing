<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>TodoMVC Javascript Tests</title>
    <link rel="stylesheet" href="../../../bower_components/jasmine/lib/jasmine-core/jasmine.css"/>
    {{#styles}}<link rel="stylesheet" href="{{.}}">{{/styles}}
  </head>
  <body>
    <div id="jasmine"></div>
    <div id="jasmine-fixture"></div>

    <!-- Jasmine dependencies -->
    <script src="../../../bower_components/jasmine/lib/jasmine-core/jasmine.js"></script>
    <script src="../../../bower_components/jasmine/lib/jasmine-core/jasmine-html.js"></script>
    <script src="../../../bower_components/jasmine/lib/jasmine-core/boot.js"></script>
    <script src="../../../bower_components/blanket/dist/qunit/blanket.js" data-cover-only="../../../app/.*"></script>
    <script src="../../../bower_components/blanket/src/adapters/jasmine-2.x-blanket.js"></script>
    <script src="/testem.js"></script>

    <!-- Source -->
    {{#serve_files}}<script src="../../../{{src}}" data-cover></script>{{/serve_files}}

  </body>
</html>
